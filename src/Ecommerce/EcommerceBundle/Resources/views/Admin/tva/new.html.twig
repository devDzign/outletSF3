{% extends '::layout/layoutAdmin.html.twig' %}

{% block body -%}
    <h1>Creation Tva</h1>

    <form action="{{ path('admin_tva_new') }}" method="POST" enctype="multipart/form-data">


        <div class="form-group">
            {{ form_label(form.nom) }}
            {{ form_errors(form.nom) }}
            {{ form_widget(form.nom,{'attr': {'class': 'form-control'}}) }}
        </div>

        <div class="form-group">
            {{ form_label(form.multiplicate) }}
            {{ form_errors(form.multiplicate) }}
            {{ form_widget(form.multiplicate,{'attr': {'class': 'form-control'}}) }}
        </div>

        <div class="form-group">
            {{ form_label(form.valeur) }}
            {{ form_errors(form.valeur) }}
            {{ form_widget(form.valeur,{'attr': {'class': 'form-control'}}) }}
        </div>


        <button type="submit" class="btn btn-default">Submit</button>
        <a href="{{ path('admin_tva_index') }}" class="btn btn-info">
            Back to the list
        </a>
        {{ form_rest(form) }}
    </form>

{% endblock %}
